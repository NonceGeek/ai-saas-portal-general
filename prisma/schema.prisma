model micro_ai_saas {
  id               BigInt    @id @default(autoincrement())
  user             String?
  prompt           String?
  task_type        String?   @default("llm")
  solution         String?
  solver           String?
  fee              BigInt?   @default(0)
  fee_unit         String?   @default("ldg")
  tx               String?   @default("")
  created_at       DateTime  @default(now()) @db.Timestamptz(6)
  solved_at        DateTime? @db.Timestamp(6)
  signature        String?
  unique_id        String?   @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  solver_type      Json?     @default("[]")
  optimized_prompt String?
}

